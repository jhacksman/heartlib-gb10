# HeartLib Web Backend Configuration

# Device for inference (cuda or cpu)
DEVICE=cuda

# Enable/disable the full pipeline (set to false for demo mode)
ENABLE_PIPELINE=true

# YingMusic-SVC path (clone from https://github.com/GiantAILab/YingMusic-SVC)
YINGMUSIC_SVC_PATH=./YingMusic-SVC

# Audio settings
SAMPLE_RATE=48000
MAX_DURATION_MS=180000

# File storage
UPLOAD_DIR=uploads
OUTPUT_DIR=outputs
MAX_UPLOAD_SIZE_MB=50
